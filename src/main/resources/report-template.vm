<!doctype html>
<html>
<head>
    <title>Migration Helper Spring Quarkus</title>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8"/>
</head>
<body>
    <h1>Migration Helper Spring Quarkus</h1>
    <h2>Blacklist</h2>
    <table>
        <tr>
            <th>Name</th>
            <th>Package</th>
            <th>Description</th>
        </tr>
        #foreach( $record in $records )
        <tr>
            <td>$record.id</td>
            <td>$record.blacklistedPackage</td>
            <td>$record.description</td>
        </tr>
        #end
    </table>
</body>
</html>